{
  "version": "0.3.0-*",
  "description": "Provides Microsoft Teams aliases (extension methods on Cake context) for Cake.",
  "copyright": "Copyright (c) WCOM AB",
  "authors": [
    "WCOM AB"
  ],
  "packOptions": {
    "summary": "Addin for Cake providing Microsoft Team aliases.",
    "projectUrl": "https://github.com/cake-contrib/Cake.MicrosoftTeams",
    "licenseUrl": "https://github.com/cake-contrib/Cake.MicrosoftTeams/blob/master/LICENSE",
    "iconUrl": "http://cdn.rawgit.com/WCOMAB/nugetpackages/master/Chocolatey/icons/wcom.png",
    "releaseNotes": "Added view action",
    "requireLicenseAcceptance": false,
    "tags": [
      "Cake",
      "Script",
      "Build",
      "Microsoft",
      "Teams"
    ],
    "owners": [
      "devlead",
      "WCOMAB",
      "cake-contrib"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/cake-contrib/Cake.MicrosoftTeams"
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "xmlDoc": true,
    "platform": "anycpu",
    "compile": {
      "includeFiles": [
        "../SolutionInfo.cs"
      ]
    }
  },
  "dependencies": {
    "Cake.Core": {
      "version": "0.17.0",
      "type": "build"
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Collections.NonGeneric": "4.0.1",
        "System.Collections.Specialized": "4.0.1"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Xml": {
          "type": "build"
        },
        "System.Net.Http": {
          "type": "build"
        }
      }
    }
  }
